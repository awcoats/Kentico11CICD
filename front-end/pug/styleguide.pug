	script.
		$(document).ready(function() {
			// Function to convert rgb value to hex
			function rgb2hex(rgb) {
				rgb = rgb.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);

				function hex(x) {
					return ("0" + parseInt(x).toString(16)).slice(-2);
				}
				return "#" + hex(rgb[1]) + hex(rgb[2]) + hex(rgb[3]);
			}
			// Get the color and convert the value to hex
			$('.sg-color').each(function() {
				var theColor = rgb2hex($(this).parent().css('color'));
				$(this).text(theColor);
			});
			// Get the font size
			$('.sg-size').each(function() {
				var theSize = $(this).parent().css('font-size');
				$(this).text(theSize);
			});
			// Get the font-family
			$('.sg-fontFam').each(function() {
				var theFontFam = $(this).parent().css('font-family');
				$(this).text(theFontFam);
			});
			// Get the font-family
			$('.sg-fontWeight').each(function() {
				var theFontWeight = $(this).parent().css('font-weight');
				$(this).text('Font-weight:' + theFontWeight);
			});
			// Get the font-family
			$('.sg-bgColor').each(function() {
				var thebackgroundColor = rgb2hex($(this).parent().css('background-color'));
				$(this).text(thebackgroundColor);
			});
		});
	script.
		//scroll effect to any internal anchor links to give visual feedback of where users are in the document and that they have not been taken to another page
		$(document).ready(function() {
			$('a[href*=\\#]').click(function(e) {
				if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
					e.preventDefault();
					var $target = $(this.hash);
					$target = $target.length && $target || $('[name=' + this.hash.slice(1) + ']');
					if ($target.length) {
						var targetOffset = $target.offset().top;
						if ($(".style-nav").length) {
							targetOffset = targetOffset - $(".style-nav").outerHeight(true);
						}
						$('html,body').animate({
							scrollTop: targetOffset
						}, 500);
						return false;
					}
				}
			});
		});
	script.
		$(document).ready(function() {
			$('body').append($("<div></div>").addClass('sg-pageMask'));

				function closeMenu() {
					$('body').removeClass('js-navActive');
					$('.sg-navToggle').removeClass('js-active');
					$('.sg-mainNav-item').removeClass('js-subActive');
				}

				function openMenu() {
					$('body').toggleClass('js-navActive');
					$('.sg-navToggle').toggleClass('js-active');
				}
				$('.sg-navToggle').click(function() {
					openMenu();
				});
				$('a.sg-mainNav-link, a.sg-mainNav-subLink, .sg-pageMask').click(function() {
					closeMenu();
				});
				$('span.sg-mainNav-link').click(function() {
					if ($(this).closest('.sg-mainNav-item').hasClass('js-subActive')) {
						$(this).closest('.sg-mainNav-item').removeClass('js-subActive');
					} else {
						$(this).closest('nav').find('.js-subActive').removeClass('js-subActive');
						$(this).closest('.sg-mainNav-item').addClass('js-subActive');
					}
				});
		});
	script.
		$(document).ready(function() {
			var topDistance = $(window).scrollTop();
			var headerHeight = $('.sg-header').outerHeight();


			$(window).scroll(function() {
				var lastPosition = $(this).scrollTop();
					if (lastPosition > topDistance) {
					// I have scrolled down
					if ($(window).scrollTop() > headerHeight) {
						// I have scrolled past the header
						$('body').removeClass('js-headerFixed--down').addClass('js-headerFixed--up');
						$('.sg-pageWrap').css('padding-top', headerHeight);
					} else {
						$('body').removeClass('js-headerFixed--down').removeClass('js-headerFixed--up');
						$('.sg-pageWrap').css('padding-top', '');
					}
				} else {
				// I have scrolled up
					if ($(window).scrollTop() >= 1) {
						$('body').addClass('js-headerFixed--down').removeClass('js-headerFixed--up');
					} else {
						$('body').removeClass('js-headerFixed--down').removeClass('js-headerFixed--up');
						$('.sg-pageWrap').css('padding-top', '');
					}
			  }
			  topDistance = lastPosition;
			});
		});
	script.
		$.fn.contrastColor = function() {
		    return this.each(function() {
		        var bg = $(this).css('background-color');
		        //use first opaque parent bg if element is transparent
		        if(bg == 'transparent' || bg == 'rgba(0, 0, 0, 0)') { 
		            $(this).parents().each(function(){
		                bg = $(this).css('background-color')
		                if(bg != 'transparent' && bg != 'rgba(0, 0, 0, 0)') return false; 
		            });
		            //exit if all parents are transparent
		            if(bg == 'transparent' || bg == 'rgba(0, 0, 0, 0)') return false;
		        }
		        //get r,g,b and decide
		        var rgb = bg.replace(/^(rgb|rgba)\(/,'').replace(/\)$/,'').replace(/\s/g,'').split(',');
		        var yiq = ((rgb[0]*299)+(rgb[1]*587)+(rgb[2]*114))/1000;
		        if(yiq >= 128) $(this).removeClass('h-colorWhite');
		        else $(this).addClass('h-colorWhite');
		    });
		};
		$(".sg-example[class*='fill']").contrastColor();