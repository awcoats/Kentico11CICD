							#{h_tags} Heading #{h_nums}
									span.sg-color
									span.sg-size
									span.sg-fontFam
									span.sg-fontWeight
						pre.sg-code(rel='HTML')
							each _, index in Array(6)
								- var n = (index+1)
								- var h_tags = '<h'+n+'>...</h'+n+'>'
								| #{h_tags}
								br
